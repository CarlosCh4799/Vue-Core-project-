 <button class=\"mr-4 button is-success is-outlined is-rounded is-normal\" @click.prevent=\"empleados\">Empleados</button>\r\n        <button class=\"mr-4 button is-success is-outlined is-rounded is-normal\" @click.prevent=\"materiales\">Materiales</button>\r\n        <button class=\"button is-success is-outlined is-rounded is-normal\" @click.prevent=\"clientes\">Clientes</button>\r\n    </div>\r\n    <br><br> \r\n    <h5 class=\"container\">Lista de Trabajos</h5>\r\n      <br>\r\n    <div class=\"container\">\r\n      <table class=\"table table-hover table-secondary\">\r\n        <thead class=\"has-background-primary-dark\">\r\n          <tr> \r\n            <th scope=\"col\">ID</th>\r\n            <th scope=\"col\">Nombre</th>\r\n            <th scope=\"col\">Descripcion</th>\r\n            <th scope=\"col\">Fecha Limite</th>\r\n            <th scope=\"col\">Material</th>\r\n            <th scope=\"col\">Cantidad Material</th>\r\n            <th scope=\"col\">Encargado</th>\r\n            <th scope=\"col\">Cliente</th>\r\n            <th scope=\"col\">Estado</th>\r\n            <th scope=\"col\">Opciones</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"item in jobs\" v-bind:key='item.id'>\r\n            <th scope=\"row\" class=\"text-secondary\">{{item.id}}</th>\r\n            <td>{{item.data.nombre}}</td>\r\n            <td>{{item.data.descripcion}}</td>\r\n            <td>{{item.data.fecha}}</td>\r\n            <td>{{item.data.material}}</td>\r\n            <td>{{item.data.cantidad}}</td>\r\n            <td>{{item.data.encargado}}</td>\r\n            <td>{{item.data.cliente}}</td>\r\n            <td>{{item.data.estado}}</td>\r\n            <td><button class=\"button is-primary is-rounded is-small is-light is-fullwidth\" @click.prevent=\"frmTrabajo\">Editar</button>\r\n            <button class=\"button is-danger is-rounded is-small is-light is-fullwidth\" @click.prevent=\"frmTrabajo\">Eliminar</button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n    <br><br>\r\n    <div class=\"container\">\r\n        <button class=\"button is-primary is-rounded is-normal\" @click.prevent=\"frmTrabajo\">Agregar Trabajo</button>\r\n    </div>\r\n    <div v-if=\"vis\">\r\n      <button @click.prevent=\"verificarD\">verificar</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport firebase from 'firebase';\r\n//import FormTrabajo from \"./Forms/FormTrabajo.vue\"\r\nlet db = firebase.firestore();\r\nlet jobsRef = db.collection('jobs');\r\n\r\n\r\nexport default {\r\n  //components: { FormTrabajo },\r\n  methods: {\r\n      clientes(){\r\n          this.$router.push({name: 'Clientes'})\r\n      },\r\n      empleados(){\r\n          this.$router.push({name: 'Empleados'})\r\n      },\r\n      materiales(){\r\n          this.$router.push({name: 'Materiales'})\r\n      },\r\n      frmTrabajo(){\r\n          this.$router.push({name: 'FormTrabajo'})\r\n      },\r\n      verificarD(){\r\n        this.fechaAsignada = this.$store.getters.getFechaG;\r\n        this.nombreAsig = this.$store.getters.getNombreG;\r\n        console.log(this.nombreAsig);\r\n        this.jobs.forEach(x => {\r\n          if(x.data.encargado == this.nombreAsig){\r\n            //if(x.data.fecha<)\r\n            console.log(this.event_at.format('YYYY-MM-DDT'));\r\n          }\r\n        });\r\n      },\r\n      /*datosForm(value){\r\n        this.fechaAsignada = value,\r\n        console.log(\"metodo ejecutado\")\r\n      }*/\r\n  },\r\n  firebase: {\r\n    jobs:jobsRef\r\n  },\r\n  mounted() {\r\n    this.jobs=[]\r\n    jobsRef.get()\r\n    .then( (r) => r.docs.map((item) => this.jobs.push({id:item.id ,data:item.data()})))\r\n  },\r\n  data(){\r\n    return{\r\n      jobs:[],\r\n      fechaAsignada: '',\r\n      nombreAsig: '',\r\n      calculo: false,\r\n      valorAEnviar: null,\r\n      vis: true,\r\n      event_at: new Date()\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n"]}]}